{
    "spec": "webgl",
    "source": "https://www.khronos.org/registry/webgl/specs/latest/1.0/",
    "levels": [ 1 ],
    "local": "webgl.html",
    "dontFetch": true,
    "dontExtract": true,
    "dontExtractReason": "definitions of IDL require manual editing, about which see comment in IDL file",
    "helpers": [
        "WebGLActiveInfo",
        "WebGLBuffer",
        "WebGLFramebuffer",
        "WebGLObject",
        "WebGLProgram",
        "WebGLRenderbuffer",
        "WebGLShader",
        "WebGLShaderPrecisionFormat",
        "WebGLTexture",
        "WebGLUniformLocation"
    ],
    "instances": {
        "WebGLActiveInfo": "getWebGLActiveInfo()",
        "WebGLBuffer": "getWebGLBuffer()",
        "WebGLContextEvent": "document.createEvent('WebGLContextEvent')",
        "WebGLFramebuffer": "getWebGLFramebuffer()",
        "WebGLObject": "getWebGLObject()",
        "WebGLProgram": "getWebGLProgram()",
        "WebGLRenderbuffer": "getWebGLRenderbuffer()",
        "WebGLRenderingContext": "document.createElement('canvas').getContext('webgl')",
        "WebGLRenderingContextBase": "document.createElement('canvas').getContext('webgl')",
        "WebGLShader": "getWebGLShader()",
        "WebGLShaderPrecisionFormat": "getWebGLShaderPrecisionFormat()",
        "WebGLTexture": "getWebGLTexture()",
        "WebGLUniformLocation": "getWebGLUniformLocation()"
    }
}
