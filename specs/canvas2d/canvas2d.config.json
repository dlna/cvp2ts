{
    "spec": "canvas2d",
    "source": "http://www.w3.org/html/wg/drafts/2dcontext/html5_canvas_CR/",
    "local": "canvas2d.html",
    "helpers": [
        "CanvasPattern"
    ],
    "instances": {
        "CanvasRenderingContext2D": "document.createElement('canvas').getContext('2d')",
        "CanvasDrawingStyles": "document.createElement('canvas').getContext('2d')",
        "CanvasPathMethods": "document.createElement('canvas').getContext('2d')",
        "CanvasGradient": "document.createElement('canvas').getContext('2d').createLinearGradient(0,0,1,1)",
        "CanvasPattern": "getCanvasPattern()",
        "TextMetrics": "document.createElement('canvas').getContext('2d').measureText('text')",
        "ImageData": "document.createElement('canvas').getContext('2d').createImageData(1,1)",
        "MouseEvent": "document.createEvent('MouseEvent')"
    }
}
