{
    "spec": "dom4",
    "source": "http://w3c.github.io/dom/",
    "local": "dom4.html",
    "helpers": [
        "MutationRecord"
    ],
    "instances": {
        "Attr": "document.getElementById('idl').attributes[0]",
        "CharacterData": "document.implementation.createDocument(null, '', null).createTextNode('text')",
        "ChildNode": "document.implementation.createDocument(null, '', null).createTextNode('text')",
        "Comment": "document.implementation.createDocument(null, '', null).createComment('comment')",
        "CustomEvent": "!!window['CustomEvent'] && new CustomEvent('custom')",
        "DOMError": "!!window['DOMError'] && new DOMError('error')",
        "DOMImplementation": "document.implementation",
        "DOMSettableTokenList": "undefined",
        "DOMTokenList": "document.body.classList",
        "Document": "document.implementation.createDocument(null, '', null)",
        "DocumentFragment": "!!window['DocumentFragment'] && new DocumentFragment()",
        "DocumentType": "document.implementation.createDocumentType('none', '', '')",
        "Element": "document.documentElement",
        "Event": "document.implementation.createDocument(null, '', null).createEvent('Event')",
        "EventTarget": "document.implementation.createDocument(null, '', null)",
        "HTMLCollection": "document.implementation.createHTMLDocument('').body.children",
        "MutationObserver": "!!window['MutationObserver'] && new MutationObserver(function(mutations, observer){})",
        "Node": "document.implementation.createDocument(null, '', null)",
        "NodeIterator": "document.createNodeIterator(document.documentElement, NodeFilter.SHOW_ALL, null, false)",
        "NodeList": "document.body.childNodes",
        "NonDocumentTypeChildNode": "document.implementation.createDocument(null, '', null).createTextNode('text')",
        "NonElementParentNode": "document.implementation.createDocument(null, '', null)",
        "ParentNode": "document.implementation.createDocument(null, '', null)",
        "ProcessingInstruction": "document.implementation.createDocument(null, '', null).createProcessingInstruction('target', 'data')",
        "Range": "document.implementation.createDocument(null, '', null).createRange()",
        "Text": "document.implementation.createDocument(null, '', null).createTextNode('text')",
        "TreeWalker": "document.createTreeWalker(document.documentElement, NodeFilter.SHOW_ALL, null, false)",
        "XMLDocument": "document.implementation.createDocument(null, '', null)"
    },
    "instancesAsync": {
        "MutationRecord": "getMutationRecordAsync(test)"
    }
}
