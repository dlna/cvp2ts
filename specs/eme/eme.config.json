{
    "spec": "eme",
    "source": "https://w3c.github.io/encrypted-media/",
    "levels": [ 1 ],
    "local": "eme.html",
    "dontFetch": true,
    "dontExtract": true,
    "dontExtractReason": "definitions of IDL require manual editing, about which see comment in IDL file",
    "helpers1": [
        "MediaEncryptedEvent",
        "MediaKeyMessageEvent",
        "MediaKeySession",
        "MediaKeyStatusMap",
        "MediaKeySystemAccess",
        "MediaKeys"
    ],
    "instances": {
        "HTMLMediaElement": "document.createElement('video')",
        "Navigator": "window['navigator'] && window.navigator"
    },
    "instancesAsync": {
        "MediaEncryptedEvent": "getMediaEncryptedEventAsync(test)",
        "MediaKeyMessageEvent": "getMediaKeyMessageEventAsync(test)",
        "MediaKeySession": "getMediaKeySessionAsync(test)",
        "MediaKeyStatusMap": "getMediaKeyStatusMapAsync(test)",
        "MediaKeySystemAccess": "getMediaKeySystemAccessAsync(test)",
        "MediaKeys": "getMediaKeysAsync(test)"
    }
}
